generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("db.url")
}

model Admin {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  password String
  firstName  String
  lastName String?
  isSuper Boolean? @default(false)
  isDeleted Boolean @default(false)

  @@map("admins")
}
